{
  "name": "wallet-web",
  "version": "0.1.0",
  "private": true,
  "homepage" : ".",
  "dependencies": {
    "axios": "^0.18.0",
    "react": "^16.8.5",
    "react-dom": "^16.8.5",
    "react-scripts": "2.1.8"
  },
  "scripts": {
    "start": "react-scripts start",
    "build": "react-scripts build",
    "test": "react-scripts test",
    "eject": "react-scripts eject",
    "lint": "prettier --check src/**/*.js"
  }
},
  "eslintConfig": {
    "extends": "react-app"
  },
  "browserslist": [
    ">0.2%",
    "not dead",
    "not ie <= 11",
    "not op_mini all"
  ],
  "devDependencies": {
    "enzyme": "^3.9.0",
    "enzyme-adapter-react-16": "^1.11.2",
    "prettier": "^1.16.4",
    "react-test-renderer": "^16.8.5"
  },
  "jest": {
    "collectCoverageFrom": [
      "src/**/*.{js}",
      "!src/serviceWorker.js",
      "!src/index.js"
    ],
    "coverageThreshold": {
      "global": {
        "functions": 95,
        "lines": 95,
        "statements": 95
      }
    },
    "coverageReporters": [
      "text"
    ]
  }
}
